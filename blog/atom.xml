---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>this is a blot</title>
	<link href="http://thisisa.blog/blog/atom.xml" rel="self"/>
	<link href="http://thisisa.blog/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://thisisa.blog/blog</id>
	<author>
		<name>this is a blog</name>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://thisisa.blog{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://thisia.blog{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
